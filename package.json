{
  "name": "cultural-expo",
  "version": "1.0.0",
  "description": "A React web application to discover cultures around the world with modern dark theme design",
  "keywords": [
    "react",
    "cultural",
    "exploration",
    "dark-theme",
    "framer-motion",
    "tailwindcss"
  ],
  "author": "Cultural Expo Team",
  "license": "MIT",
  "homepage": "./",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run optimize",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --ci",
    "eject": "react-scripts eject",
    "serve": "npx serve -s build -l 3000",
    "optimize": "echo 'Build optimized for production!'",
    "analyze": "npx source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "framer-motion": "^12.23.12",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-intersection-observer": "^9.16.0",
    "react-leaflet": "^5.0.0",
    "react-scripts": "^5.0.1",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^30.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
